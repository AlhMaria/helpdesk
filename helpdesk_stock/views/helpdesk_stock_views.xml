<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Inherit Form view -->
  <record id="view_stock_ticket_form" model="ir.ui.view">
    <field name="name">stock.ticket.view.form helpdesk</field>
    <field name="model">helpdesk.ticket</field>
    <field name="inherit_id" ref="helpdesk.ticket_view_form"/>
    <field name="arch" type="xml">
      <field name="actions_ids" position="after">
        <field name="stock_picking_ids"/>
      </field>
    </field>
  </record>
</odoo>
